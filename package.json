    "express": "^4.17.1",
    "googleapis": "^41.0.1",
    "httpntlm": "^1.7.6",
    "jsdom-global": "^3.0.2",
    "lodash": "^4.17.15",
    "lookup-dns-cache": "^2.1.0",
    "moment": "^2.24.0",
    "request": "~2.88.0",
    "serve-favicon": "^2.3.0",
    "socket.io": "^2.2.0",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "eslint": "^6.1.0",
    "jsdom": "15.1.1",
    "jsdom-global": "3.0.2",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
