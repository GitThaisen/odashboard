{
  "name": "@nrk/odashboard",
  "homepage": "https://github.com/nrkno/odashboard",
  "description": "Configurable dashboard with simple plugin architecture",
  "author": "NRK <opensource@nrk.no> (https://www.nrk.no/)",
  "license": "MIT",
  "version": "0.0.7",
  "bin": {
    "odashboard": "bin/odashboard.js"
  },
  "scripts": {
    "preinstall": "node scripts/preinstall.js",
    "test:lint": "eslint . --ext .js --ignore-path .gitignore --cache",
    "test": "npm run test:lint --silent && mocha --recursive test/",
    "test-travis": "mocha --recursive test/",
    "build": "webpack --progress",
    "watch": "webpack --watch",
    "watch-tests": "mocha --recursive test/ --watch",
    "start": "nodemon app.js",
    "prepublish": "npm run build",
    "release": "np"
  },
  "dependencies": {
    "angular": "^1.5.8",
    "angular-route": "^1.5.8",
    "argparse": "^1.0.9",
    "chart.js": "^2.3.0",
    "ejs": "^2.0.0",
    "express": "^4.14.0",
    "googleapis": "^40.0.0",
    "httpntlm": "^1.6.1",
    "jsdom-global": "^3.0.2",
    "lodash": "^4.16.4",
    "lookup-dns-cache": "^2.1.0",
    "moment": "^2.15.1",
    "request": "~2.88.0",
    "serve-favicon": "^2.3.0",
    "socket.io": "^2.0.4",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "eslint": "^6.0.1",
    "jsdom": "15.1.1",
    "jsdom-global": "3.0.2",
    "mocha": "^6.1.4",
    "nodemon": "^1.11.0",
    "np": "^5.0.3",
    "sinon": "^7.3.2",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5"
  },
  "main": "app.js",
  "repository": {
    "type": "git",
